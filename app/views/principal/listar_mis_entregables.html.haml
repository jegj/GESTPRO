.mensajeprincipal
	-if @entregable==-1
		#vacio
			%p
				No hay entregables 	
	-else	
		#listado_entregable
			-@entregable.each do  |un_entregable|
				%h3
					=link_to "Grupo #{un_entregable.grupo_nro}"
					=link_to "Entrega: #{Entrega.where(:id => un_entregable.entrega_id).first.nombre} "
				%div
					%p
						="Fecha/Hora: #{un_entregable.fecha_hora}"
					%p
						="Version:  #{un_entregable.version}"
					%p
						="Cedula de Estudiante que Entrego : #{un_entregable.estudiante_cedula_entrego}"
					%p
						=link_to "Descargar Entregable" ,{:controller => "principal" ,:action =>"algo"},:class => "nolistado"				
%hr				
%p
.demo
	= button_to "Atras" ,:action => "bienvenida"
	
